{
    "version": 2,
    "routes": [
        {
            "builds": [
                {
                    "src": "dist/app.js",
                    "use": "@vercel/node",
                    "config": {
                        "includeFiles": [
                            "dist/**"
                        ]
                    }
                }
            ],
            "src": "/(.*)",
            "dest": "dist/app.js"
        }
    ]
}